<?xml version="1.0"?>
<rolemap>
  <roles>
    <role name="Community Member"/>
    <role name="Workspace Administrator"/>
    <role name="Workspace Member"/>
  </roles>
  <permissions>
    <permission name="Add workspaces" acquire="True">
      <role name="Manager"/>
      <role name="Community Member"/>
    </permission>
    <permission name="ATContentTypes: Add Document" acquire="True">
      <role name="Workspace Administrator"/>
      <role name="Workspace Member"/>
    </permission>
    <permission name="ATContentTypes: Add Folder" acquire="True">
      <role name="Workspace Administrator"/>
      <role name="Workspace Member"/>
    </permission>
    <permission name="Delete objects" acquire="True">
      <role name="Workspace Administrator"/>
      <role name="Workspace Member"/>
    </permission>
    <permission name="List folder contents" acquire="True">
      <role name="Workspace Administrator"/>
      <role name="Workspace Member"/>
    </permission>
    <permission name="Modify portal content" acquire="True">
      <role name="Workspace Administrator"/>
      <role name="Workspace Member"/>
    </permission>
    <permission name="View" acquire="True">
      <role name="Workspace Administrator"/>
      <role name="Workspace Member"/>
    </permission>
  </permissions>
</rolemap>
