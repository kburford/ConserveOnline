<?xml version="1.0"?>
<?oxygen RNGSchema="../../schemas/tnc.rng" type="xml"?>
<response>
  <view name="completereset-password.html" type="site" title="Reset Password" section="people" />
  <formcontroller action=".">
    <field name="form.file" widget="text" required="required">
      <label>Username</label>
    </field>
    <field name="form.password" widget="password" required="required">
      <label>New Password</label>
      <description>Password must be at least 8 characters long.</description>
    </field>
    <field name="form.password" widget="password" required="required">
      <label>Re-enter Password</label>
    </field>
    <submit name="submit">Save</submit>
    <submit name="cancel">Cancel</submit>
  </formcontroller>
  <breadcrumbs>
    <entry href="/">Home</entry>
    <entry>Reset Password</entry>
  </breadcrumbs>
</response>
